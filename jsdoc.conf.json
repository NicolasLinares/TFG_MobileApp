{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": [
        "src", 
        "./README.md"
      ],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown",
      "better-docs/component"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
          "outputSourceFiles": true,
          "includeDate": false,
          "useLongnameInNav": true
      }
    },
    "opts": {
      "template": "node_modules/docdash",
      "destination": "jsdoc",
      "recurse": true,
      "readme": "README.md"
    },
    "markdown": {
      "parser": "gfm",
      "hardwrap": true,
      "idInHeadings": true
    },
    "docdash": {
      "search": true,
      "collapse": true,
      "typedefs": true,
      "static": true,
      "disqus": "",
      "removeQuotes": "none",
      "scripts": [],
      "sectionOrder": [
           "Namespaces",
           "Modules",
           "Externals",
           "Events",
           "Mixins",
           "Tutorials",
           "Interfaces"
      ],
      "menu": {
        "Github": {
            "href":"https://github.com/NicolasLinares/TFG_MobileApp",
            "target":"_blank",
            "class":"menu-item",
            "id":"github_link"
        }
      }
    }
  }



